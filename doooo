local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local MianTab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MianTab:AddToggle({
	Name = "anti afk 1.0",
	Default = true,
	Callback = function(Value)
		task.wait(5)
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/Anti-afk-1.0/main/Anti%20afk%201.0"))()
	end    
})

MianTab:AddToggle({
	Name = "anti afk",
	Default = true,
	Callback = function(Value)
		task.wait(6)
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/anti-afk-/main/darkscripts"))()
	end    
})

MianTab:AddToggle({
	Name = "fuse poodle",
	Default = true,
	Callback = function(Value)
		task.wait(7)
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/Potion-poodle/main/Autofuse"))()
	end    
})

MianTab:AddToggle({
	Name = "Teleport",
	Default = true,
	Callback = function(Value)
		task.wait(8)
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/auto-tp/main/main%20script"))()
	end    
})

MianTab:AddToggle({
	Name = "anti lag",
	Default = true,
	Callback = function(Value)
		task.wait(9)
            loadstring(game:HttpGet("https://pastebin.com/raw/8YZ2cc6V"))()
	end    
})

OrionLib:Init()

